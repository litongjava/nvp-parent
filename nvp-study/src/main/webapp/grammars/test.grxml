<?xml version="1.0" encoding="UTF-8"?>
<!-- root="_ROOT_":指定入口 -->
<grammar version="1.0" xmlns="http://www.w3.org/2001/06/grammar" tag-format="swi-semantics/1.0"
         xml:lang="zh-CN" root="ROOT">
    <rule id="ROOT">
        <tag>dm_root='yes'</tag>
        <item repeat="0-1">
            <item repeat="0-1">要一个</item>
            大的
        </item>
        麻辣香锅
        <item repeat="0-">
            <one-of>
                <item>配</item>
                <item>加</item>
            </one-of>
        </item>
    </rule>
</grammar>