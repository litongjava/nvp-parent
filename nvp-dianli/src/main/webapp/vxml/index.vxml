    <form id="A7_2_1">
        <field name="A7_2_1" type="digits">
            <prompt>请输入密码并按井号键确认</prompt>
            <grammar src="../grammars/query1.grxml"/>
            <filled>
                <if cond="A7_2_1==12345678">
