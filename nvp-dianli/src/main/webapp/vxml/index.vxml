    <form id="A8">
        <block name="A8">
            <prompt>业务建设中,目前还没有对外开放,请选择其它服务</prompt>
            <goto next="#success"/>
        </block>
    </form>
    <form id="A9">
        <block name="A9">
            <goto next="#transferMan"/>
        </block>
    </form>

    <form id="A0">
        <block name="A0">
            <goto next="#transferMan"/>
        </block>
    </form>

    <var name="successP" expr="'请问您还需要办理什么业务？
                    1,查询电费余额,
                    2,查询计划停电信息,
                    3,查询阶梯电价,
                    4,查询消费记录,
                    5,故障报修,
                    6,修改密码,
                    7,紧急锁卡,紧急解卡,
                    8,业务办理,
                    9,举报投诉,
                    0,人工服务,'"/>
    <var name="catchSuccessP" expr="'我没有理解你的意思,你可以这样问问题,
                    1,查询电费余额,
                    2,查询计划停电信息,
                    3,查询阶梯电价,
                    4,查询消费记录,
                    5,故障报修,
                    6,修改密码,
                    7,紧急锁卡,紧急解卡,
                    8,业务办理,
                    9,举报投诉,
                    0,人工服务'"/>

    <form id="success">
        <field name="success" type="digits?language=zh-CN;length=1">
            <prompt><value expr="successP"/></prompt>
            <grammar src="../grammars/query1.grxml"/>
            <filled>
                <!--电费余额-->
                <if cond="success.dm_root=='findPowerRate' || success.dm_root=='matchOne'">
                    <goto next="#A1"/>
                </if>
                <if cond="success==1">
                    <goto next="#A1"/>
                </if>

                <!--停电信息-->
                <if cond="success.dm_root=='findPowerCut'">
                    <goto next="#A2"/>
                    <elseif cond="success.dm_root=='matchTwo'"/>
                    <goto next="#A2"/>
                </if>

                <if cond="success==2">
                    <goto next="#A2"/>
                </if>

                <!--查询阶梯电价-->
                <if cond="success.dm_root=='findLadderPrice'">
                    <goto next="#A3"/>
                    <elseif cond="success.dm_root=='matchThree'"/>
                    <goto next="#A3"/>
                </if>

                <if cond="success==3">
                    <goto next="#A3"/>
                </if>
                <!--查询消费记录-->
                <if cond="success.dm_root=='findConsumeRecord'">
                    <goto next="#A4"/>
                    <elseif cond="success.dm_root=='matchFour'"/>
                    <goto next="#A4"/>
                </if>

                <if cond="success==4">
                    <goto next="#A4"/>
                </if>

                <!--5,故障报修,-->
                <if cond="success.dm_root=='powerRepair' ||success.dm_root=='matchFive'">
                    <goto next="#A5"/>
                </if>

                <if cond="success==5">
                    <goto next="#A5"/>
                </if>
                <!--6,修改密码,-->
                <if cond="success.dm_root=='changePassword' || success.dm_root=='mcatchSix'">
                    <goto next="#A6"/>
                </if>

                <if cond="success==6">
                    <goto next="#A6"/>
                </if>
                <!--7,紧急锁卡,紧急解卡,-->
                <if cond="success.dm_root=='unlockCard' || success.dm_root=='lockCard' || success.dm_root=='matchSeven'">
                    <goto next="#A7"/>
                </if>

                <if cond="success==7">
                    <goto next="#A7"/>
                </if>
                <!--8,业务办理,-->
                <if cond="success.dm_root=='businessManagement' || success.dm_root=='matchEight'">
                    <goto next="#A8"/>
                </if>

                <if cond="success==8">
                    <goto next="#A8"/>
                </if>
                <!--9,举报投诉,-->
                <if cond="success.dm_root=='reportComplaint' || success.dm_root=='matchNine'">
                    <goto next="#A9"/>
                </if>

                <if cond="success==9">
                    <goto next="#A9"/>
                </if>
                <!--0,人工服务,-->
                <if cond="success.dm_root=='manualService'|| success.dm_root=='matchZero'">
                    <goto next="#A0"/>
                </if>

                <if cond="success==0">
                    <goto next="#A0"/>
                </if>

                <if cond="success.dm_root=='end'">
                    <goto next="#end"/>
                </if>
                <prompt>我没有理解你的意思</prompt>
                <goto next="#success"/>
            </filled>

            <catch event="noinput nomatch" count="1">
                <prompt>
                    <value expr="catchSuccessP"/>
                </prompt>
            </catch>

            <catch event="noinput nomatch" count="2">
                <prompt>
                    <value expr="catchSuccessP"/>
                </prompt>
            </catch>

            <catch event="noinput nomatch" count="3">
                <prompt>感谢你的致电,再见</prompt>
                <exit/>
            </catch>
        </field>
    </form>

    <!--转人工-->
    <form id="transferMan">
        <transfer name="call" dest="4" bridge="false">
            <prompt>您的电话正在被转接到话务员，请稍候…</prompt>
            <filled>
                <if cond="call == noanswer">
                    <prompt>对不起，话务员无法接听您的电话，请稍后再试。谢谢，再见！</prompt>
                    <disconnect/>
                    <exit/>
                </if>
            </filled>
        </transfer>
    </form>

    <form id="end">
        <block>
            感谢你的致电,再见
            <exit/>
        </block>
    </form>
</vxml>